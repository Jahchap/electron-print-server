{
  "name": "electron-print-server",
  "description": "Print server for printing URLs on local printers",
  "author": "jbot <monstion@gmail.com>",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "build": {
    "productName": "Electron print server",
    "win": {
      "target": [ "portable", "nsis" ],
      "extraResources": "./external/SumatraPDFx64.exe"
    },
    "linux": {
      "target": [ "tar.gz", "deb" ],
      "category": "Printing"
    }
  },
  "dependencies": {
    "source-map-support": "0.5.3"
  },
  "devDependencies": {
    "@types/express": "4.11.1",
    "body-parser": "^1.18.3",
    "debug": "^4.0.1",
    "electron": "2.0.7",
    "electron-builder": "20.28.1",
    "electron-settings": "^3.2.0",
    "electron-webpack": "2.1.2",
    "electron-webpack-vue": "2.1.2",
    "express": "4.16.2",
    "lodash": "^4.17.11",
    "tmp": "^0.0.33",
    "vue": "2.5.17",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.16.5",
    "webpack-build-notifier": "0.1.22"
  }
}
